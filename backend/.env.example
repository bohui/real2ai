# Environment Configuration
ENVIRONMENT=development
DEBUG=true

# Database
SUPABASE_URL=https://your-project.supabase.co
SUPABASE_ANON_KEY=your-anon-key
SUPABASE_SERVICE_KEY=your-service-key
DATABASE_URL=postgresql://postgres:password@localhost:54322/postgres

# AI Services
OPENAI_API_KEY=sk-your-openai-key
LANGSMITH_API_KEY=your-langsmith-key
LANGSMITH_PROJECT=real2ai-development

# External APIs
STRIPE_SECRET_KEY=sk_test_your-stripe-key
STRIPE_PUBLISHABLE_KEY=pk_test_your-stripe-key
DOMAIN_API_KEY=your-domain-api-key
CORELOGIC_API_KEY=your-corelogic-key

# Redis Cache
REDIS_URL=redis://localhost:6379

# Security
JWT_SECRET_KEY=your-jwt-secret-key
JWT_ALGORITHM=HS256
JWT_EXPIRATION_HOURS=24

# File Storage
MAX_FILE_SIZE=52428800
ALLOWED_FILE_TYPES=pdf,doc,docx

# Monitoring
SENTRY_DSN=https://your-sentry-dsn
LOG_LEVEL=INFO

# Australian Specific
DEFAULT_AUSTRALIAN_STATE=NSW
ENABLE_STAMP_DUTY_CALCULATION=true
ENABLE_COOLING_OFF_VALIDATION=true