# Production environment variables for Cloudflare Pages
VITE_APP_NAME=Real2.AI
VITE_APP_VERSION=1.0.0
VITE_NODE_ENV=production

# API Configuration - Update these URLs to match your production API
VITE_API_BASE_URL=https://api.real2.ai
VITE_WS_BASE_URL=wss://api.real2.ai

# Feature flags
VITE_ENABLE_ANALYTICS=true
VITE_ENABLE_SENTRY=false
VITE_DEBUG_MODE=false

# Cloudflare specific
VITE_DEPLOYMENT_TARGET=cloudflare-pages